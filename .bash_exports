#!/bin/sh

export PATH=~/bin:$PATH:/opt/android-sdk/tools:/opt/android-sdk/platform-tools
export EDITOR="vim"
export BROWSER="xdg-open"

# Use system anti-aliased fonts for java.
export _JAVA_OPTIONS='-Dawt.useSystemAAFontSettings=on -Dswing.aatext=true'

# Warn the Java VM that xmonad is non-reparenting.
export _JAVA_AWT_WM_NONREPARENTING=1

# Force GTK style (Qt5). Install qt5-styleplugins for this.
export QT_STYLE_OVERRIDE=gtk2

# vim: set filetype=sh:
